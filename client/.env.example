# WalletConnect Project ID (required)
# Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Alchemy API Key (recommended)
# Get one at https://www.alchemy.com/
NEXT_PUBLIC_ALCHEMY_ID=your_alchemy_api_key_here 

# GraphQL Endpoint for The Graph (required for loading placed items)
# This should point to your deployed subgraph
NEXT_PUBLIC_GRAPHQL_ENDPOINT=https://api.thegraph.com/subgraphs/name/your-subgraph-name 